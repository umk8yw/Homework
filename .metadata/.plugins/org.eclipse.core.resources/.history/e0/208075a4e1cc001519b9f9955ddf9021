package com.atraxo.homework5;

import java.util.ArrayList;

public class Ex1 {

	public static void main(String[] args) {
		ArrayList<Integer> a = new ArrayList<Integer>();
		a.add(1);
		a.add(3);
		a.add(5);
		ArrayList<Integer> b = new ArrayList<Integer>();
		b.add(2);
		b.add(4);
		b.add(6);

		for (Integer i : b) {
			a.add(i - 1, i);
		}

		for (Integer i : a)
			System.out.println(i);
	}
}
