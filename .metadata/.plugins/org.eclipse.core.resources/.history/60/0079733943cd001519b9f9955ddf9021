package com.atraxo.homework6;

public class Ex3<T> {

	public void bubbleSort(Collection<T> array) {

	    int n = array.;
	    int temp = 0;

	    for (int i = 0; i < n; i++) {
	        for (int j = 1; j < (n - i); j++) {

	            if (array[j - 1] > array[j]) {
	                temp = array[j - 1];
	                array[j - 1] = array[j];
	                array[j] = temp;
	            }

	        }
	    }
	}
}
