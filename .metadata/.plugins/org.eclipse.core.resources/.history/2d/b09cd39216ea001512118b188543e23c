package com.atraxo.homework10.Ex3;

import java.net.Socket;

public class ChatClient {

	public static void main(String[] args) {
		try(Socket socket = new Socket("localhost", 6543)){
			while(true) {
				System.out.println("alive");
				Thread.sleep(500);
			}
		}
		catch (Exception e) {
			e.printStackTrace();
		}
	}
}
